{
  "name": "vcard-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build:staging": "NODE_ENV=staging nuxt build",
    "build:development": "export NODE_OPTIONS=--openssl-legacy-provider && NODE_ENV=development nuxt build",
    "build:production": "NODE_ENV=production nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.0",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "chart.js": "^2.9.3",
    "dayjs": "^1.8.32",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.20",
    "nuxt": "^2.14.0",
    "vue-clipboard2": "^0.3.1",
    "vue-quick-edit": "^1.3.0",
    "vue-select": "^3.10.8",
    "vue2-datepicker": "^3.6.1",
    "vuedraggable": "^2.24.3",
    "vuelidate": "^0.7.5",
    "vuescroll": "^4.17.3",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "10.1.0",
    "css-loader": "^5.0.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^6.0.1",
    "postcss": "^8.1.10",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.0.5",
    "sass": "1.32.12",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "^20.0.0"
  }
}
